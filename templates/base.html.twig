<!DOCTYPE html>
<html lang="zxx">

<head>

        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta charset="UTF-8">
        <meta name="description" content="Cloud 83 - hosting template ">
        <meta name="keywords" content="cloud, hosting, creative, html">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {% block stylesheets %}
        <!-- Favicon -->
        <link href="img/favicon.ico" rel="shortcut icon"/>

        <!-- Google Font -->
        <link href="https://fonts.googleapis.com/css?family=Montserrat:400,400i,500,500i,600,600i,700,700i" rel="stylesheet">


        <!-- Stylesheets -->
        <link rel="stylesheet" href="{{ asset ('css/bootstrap.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset ('css/font-awesome.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset ('css/flaticon.css') }}"/>
        <link rel="stylesheet" href="{{ asset ('css/owl.carousel.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset ('css/animate.css') }}"/>
        <link rel="stylesheet" href="{{ asset ('css/style.css') }}"/>

        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

    {% endblock %}
</head>
<body>


        <!-- Page Preloder -->
        <div id="preloder">
            <div class="loader"></div>
        </div>

        <!-- Header section -->
        <header class="header-section">
            <a href="./base.html.twig" class="site-logo"><img src="" alt=""></a>
            <div class="nav-switch">
                <i class="fa fa-bars"></i>
            </div>
            <div class="nav-warp">
                <div class="user-panel">
                    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                        <a href="{{ path('logout') }}">Logout</a>
                    {% else %}
                        <a href="{{ path('login') }}">Login</a> /
                        <a href="{{ path('register') }}">Register</a>
                    {% endif %}

                </div>
                <ul class="main-menu">
                    <ul>
                        <li><a href="{{ path('home') }}">Home</a></li>
                        <li><a href="{{ path('list_conferences') }}">Conference</a></li>
                        <li><a href="{{ path('user') }}">User</a></li>
                        {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                            <li><a href="{{ path('profile') }}">Profil</a></li>
                            <li><a href="{{ path('admin') }}">Admin</a></li>
                        {% endif %}
                        {#{{ render(controller(#}
                        {#'App\\Controller\SearchController::searchBar'#}
                        {#)) }}#}
                    </ul>
                </ul>
            </div>
        </header>
        <!-- Header section end -->


{% block body %}{% endblock %}


<!-- Footer section -->
<footer class="footer-section">
    {% block footer %}
    <div class="container">
        <div class="footer-widget about-widget">
            <div class="fw-social social">
                <a href="#"><i class="fa fa-facebook"></i></a>
                <a href="#"><i class="fa fa-twitter"></i></a>

            </div>
            <br>
        </div>
        <ul class="footer-menu">
                <li><a href="{{ path('home') }}">Home</a></li>
                <li><a href="{{ path('list_conferences') }}">Conference</a></li>
                <li><a href="{{ path('user') }}">User</a></li>
                {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                    <li><a href="{{ path('profile') }}">Profil</a></li>
                    <li><a href="{{ path('admin') }}">Admin</a></li>
                {% endif %}
                {#{{ render(controller(#}
                {#'src:Post:searchBar'#}
                {#)) }}#}
        </ul>
        <div class="copyright"><!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
            Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | <i class="fa fa-heart-o" aria-hidden="true"></i> by Lucie</a>
            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. --></div>
    </div>
    {% endblock %}
</footer>
<!-- Footer top section end -->

{% block javascripts %}
    <!--====== Javascripts & Jquery ======-->
    <script src="js/jquery-3.2.1.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/circle-progress.min.js"></script>
    <script src="js/main.js"></script>
    <script src="//code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>
    <script src="{{ asset('bundles/pugxautocompleter/js/autocompleter-jqueryui.js') }}"></script>


{% endblock %}
</body>
</html>
